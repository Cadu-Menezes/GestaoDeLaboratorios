    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.marca)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.processador)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.placaMae)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.memoria)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.hd)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numeroPratrimonio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.dataCompra)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.id">Details</a> |
